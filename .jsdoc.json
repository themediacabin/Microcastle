{
  "source": {
    "include": ["src", "package.json", "README.md"],
    "exclude": [],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": ["node_modules/jsdoc-strip-async-await"],
  "opts": {
    "destination": "./docs/",
    "encoding": "utf8",
    "private": false,
    "recurse": true,
    "template": "./node_modules/loke-jsdoc-theme"
  }
}