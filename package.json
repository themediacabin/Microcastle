    "immutable": "^3.8.1",
    "lodash": "^4.13.1",
    "marked": "^0.3.5",
    "react": "15.1.0",
    "react-dnd": "^2.1.4",
    "react-dnd-html5-backend": "^2.1.2",
    "react-dom": "^15.1.0",
    "react-icons": "^2.1.0",
    "react-redux": "^4.4.5",
